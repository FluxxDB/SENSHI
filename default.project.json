{
	"name": "SENSHI",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],
	"tree": {
		"$className": "DataModel",
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"Framework": {
				"$className": "Folder",
				"$path": "src/server"
			}
		},
		"ServerStorage": {
			"$className": "ServerStorage",
			"ServerPackages": {
				"$className": "Folder",
				"$path": "ServerPackages"
			}
		},
		"Players": {
			"$className": "Players",
			"$properties": {
				"CharacterAutoLoads": false
			}
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"types": {
				"$path": "src/types"
			},
			"Client": {
				"$className": "Folder",
				"$path": "src/client"
			},
			"Shared": {
				"$className": "Folder",
				"$path": "src/shared"
			},
			"Packages": {
				"$path": "Packages"
			},
			"Vendor": {
				"$path": "src/vendor"
			}
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"$properties": {
				"HealthDisplayDistance": 0,
				"NameDisplayDistance": 0,
				"CameraMode": "Classic",
				"DevCameraOcclusionMode": "Invisicam",
				"DevComputerCameraMovementMode": "UserChoice",
				"DevTouchCameraMovementMode": "UserChoice",
				"LoadCharacterAppearance": false,
				"UserEmotesEnabled": false,
				"DevComputerMovementMode": "UserChoice",
				"DevTouchMovementMode": "UserChoice",
				"EnableMouseLockOption": false,
				"GameSettingsAvatar": "R15",
				"GameSettingsR15Collision": "InnerBox"
			},
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"runtime": {
					"$path": "src/runtime.client.lua"
				}
			}
		},
		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		}
	}
}